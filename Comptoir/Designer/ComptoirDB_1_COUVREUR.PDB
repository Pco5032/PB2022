     table(select DNF_UTIL.split(regenatsp) from ilot \
           where ea=i.ea and ua=i.ua and prop=i.prop and comp=i.comp and il=i.il) t\
where i.prop='2010' and regenatsp is not null\
order by ea,ua,prop,comp,il


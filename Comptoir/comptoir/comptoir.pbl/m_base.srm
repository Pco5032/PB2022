type m_autorisationdetransportformulairevierge from menu within m_impressions
end type

event clicked;str_Params lstr_Params
string	ls_text, ls_document
integer	li_nbre
boolean	lb_correct

ls_document=profileString(gs_inifile, "formulaires", "autoTrsp", "")
IF f_isEmptyString(ls_document) THEN
	gu_message.uf_error("Le nom du document n'est pas spécifié dans les paramètres de configuration (" + &
	gs_inifile + ")")
	return
END IF

ls_document = gs_cenpath + "\doc\" + ls_document
IF NOT fileExists(ls_document) THEN
	gu_message.uf_error("Le document " + ls_document + " est introuvable")
	return
END IF
lstr_Params.a_param[1]  = "Introduisez le nombre d'exemplaire(s) à imprimer"

lb_correct = FALSE
do
	OpenWithParm(W_GetText, lstr_Params)
	IF Message.DoubleParm = -1 THEN
		return // sort de la fonction
	END IF
	ls_text = Message.StringParm

	IF f_IsEmptyString(ls_text) OR NOT isnumber(ls_text) THEN
		gu_message.uf_error("Veuillez introduire une valeur numérique")
		CONTINUE
	ELSE
		li_nbre = integer(ls_text)
	END IF
	IF li_nbre <= 0 THEN
		gu_message.uf_error("Veuillez introduire une valeur > 0")
		CONTINUE
	END IF
	IF gu_message.uf_query("Confirmez-vous l'impression de " + f_string(li_nbre) + &
		" exemplaire(s)", YesNo!, 2) = 1 THEN
		lb_correct = TRUE
	ELSE
		CONTINUE
	END IF
loop until lb_correct

IF lb_correct THEN
	f_print_word(ls_document, li_nbre)
END IF
end event


forward prototypes
public subroutine mf_enableaction (string as_itemstoenable[])
public subroutine mf_hideaction (string as_itemstohide[])
public subroutine mf_showaction (string as_itemstoshow[])
public subroutine mf_otheritems ()
end prototypes

public subroutine mf_enableaction (string as_itemstoenable[]);// disabler toutes les options du menu action (mais pas le menu action lui-même)
mf_disablemenu(this.m_action, FALSE)

// enabler les options voulues dans le menu action
mf_enableitems(this.m_action, as_itemstoenable)
end subroutine

public subroutine mf_hideaction (string as_itemstohide[]);// cacher les options voulues dans le menu action
mf_hideitems(this.m_action, as_itemstohide)
end subroutine

public subroutine mf_showaction (string as_itemstoshow[]);// afficher les options voulues dans le menu action
mf_showitems(this.m_action, as_itemstoshow)
end subroutine

public subroutine mf_otheritems ();// cocher ou pas l'option "traçage SQL" quand on change de menu, en fonction de l'état de la variable 'gb_sqlspy_on'
m_outils.m_sqlspy.checked = gb_sqlspy_on

end subroutine


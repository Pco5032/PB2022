rString:=Translate(rString,'«','C');\
rString:=Translate(rString,'ËÈÍÎ','eeee');\
rString:=Translate(rString,'»… À','EEEE');\
rString:=Translate(rString,'ÏÌÓÔ','iiii');\
rString:=Translate(rString,'ÃÕŒœ','IIII');\
rString:=Translate(rString,'Ò','n');\
rString:=Translate(rString,'—','N');\
rString:=Translate(rString,'ÚÛÙıˆ¯','oooooo');\
rString:=Translate(rString,'“”‘’÷ÿ','OOOOOO');\
rString:=Translate(rString,'˘˙˚¸','uuuu');\
rString:=Translate(rString,'Ÿ⁄€‹','UUUU');\
rString:=Translate(rString,'˝','y');\

CREATE OR REPLACE TRIGGER DNF_DBALIAS_CRYPT\
BEFORE INSERT OR UPDATE\
OF "SCHEMA" ,PASSWD\
ON DNF_DBALIAS\
FOR EACH ROW\
begin \
 if :new.passwd != :old.passwd OR :old.passwd is NULL \
  then \
   :new.passwd:=dnf_encrypt_raw(:new.passwd,DNF_vars.getCryptKey()); \
 end if; \
 if :new.schema != :old.schema OR :old.schema is NULL \
  then \
   :new.schema:=dnf_encrypt_raw(:new.schema,DNF_vars.getCryptKey()); \
 end if; \
end;\

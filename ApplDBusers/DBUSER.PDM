-- fonction de dÈcryptage\
CREATE OR REPLACE FUNCTION dnf_decrypt_raw(v_inputraw IN RAW, v_keystring IN varchar2) \
RETURN varchar2\
IS\
  v_key8 VARCHAR2(8);\
  v_decryptedraw VARCHAR2(2048);\
  v_rawinput RAW(128);\
  v_rawkey RAW(128);\
BEGIN\
if v_inputraw IS NOT NULL\
then\
  v_key8 := rpad(v_keystring,8,' ');\
  v_rawkey := UTL_RAW.CAST_TO_RAW(v_key8);\
  dbms_obfuscation_toolkit.DESDecrypt(input => v_inputraw, \
               key => v_rawkey, decrypted_data => v_decryptedraw);\
  return(rtrim(UTL_RAW.CAST_TO_VARCHAR2(v_decryptedraw)));\
else\
  return(NULL);\
end if;\
END;\
\
-- fonction de cryptage\
CREATE OR REPLACE FUNCTION dnf_encrypt_raw(v_inputstring IN varchar2, v_keystring IN varchar2) \
RETURN RAW\
IS\
  v_string8 VARCHAR2(2048);\
  v_key8 VARCHAR2(8);\
  v_encryptedraw VARCHAR2(2048);\
  v_rawinput RAW(128);\
  v_rawkey RAW(128);\
BEGIN\
if v_inputstring IS NOT NULL\
then\
  v_string8 := RPAD(v_inputstring,(8*ROUND(LENGTH(v_inputstring)/8,0)+8));\
  v_key8 := rpad(v_keystring,8,' ');\
  v_rawinput := UTL_RAW.CAST_TO_RAW(v_string8);\
  v_rawkey := UTL_RAW.CAST_TO_RAW(v_key8);\
  dbms_obfuscation_toolkit.DESEncrypt(input => v_rawinput, \
               key => v_rawkey, encrypted_data => v_encryptedraw );\
  return(v_encryptedraw);\
else\
  return(NULL);\
end if;\
END;\

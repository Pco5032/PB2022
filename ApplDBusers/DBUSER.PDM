create or replace type varchar_array as table of varchar2(100);\
/\
CREATE OR REPLACE PACKAGE dnf_util\
AS\
  PROCEDURE p_refresh_snapshot(list varchar2, method varchar2:= '?');\
  PROCEDURE p_compile_invalid_views;\
  FUNCTION split(p_list varchar2, p_delimiter varchar2 := ',') \
           return varchar_array pipelined;\
  FUNCTION join (p_cursor IN SYS_REFCURSOR)\
           return  VARCHAR2;\
  FUNCTION isnumber(p_val IN VARCHAR2) \
           return NUMBER;\
  FUNCTION removeAccent(myString IN VARCHAR2) return varchar2;\
END dnf_util;\
/\
CREATE OR REPLACE PACKAGE BODY dnf_util\
AS\
FUNCTION join(p_cursor IN SYS_REFCURSOR)\
  return  VARCHAR2\
IS\
  l_return  VARCHAR2(32767); \
  l_temp    VARCHAR2(32767);\
BEGIN\
  loop\
    fetch p_cursor into l_temp;\
    exit when p_cursor%NOTFOUND;\
    l_return := l_return || ',' || l_temp;\
  end loop;\
  close p_cursor;\
  return ltrim(l_return, ',');\
END join;\
\
FUNCTION split(p_list varchar2, p_delimiter varchar2 := ',') \
         return varchar_array pipelined\
IS\
    l_idx    pls_integer;\
    l_list   varchar2(32767):= p_list;\
    l_value  varchar2(32767);\
BEGIN\
    loop\
        l_idx := instr(l_list, p_delimiter);\
        if l_idx > 0 then\
            pipe row(substr(l_list, 1, l_idx-1));\
            l_list:= substr(l_list, l_idx + length(p_delimiter));\
        else\
            pipe row(l_list);\
            exit;\
        end if;\
    end loop;\
    return;\
END split;\
\
FUNCTION isnumber(p_val IN VARCHAR2) \
         return NUMBER\
IS\
    l_val NUMBER;\
BEGIN\
    l_val := TO_NUMBER(p_val); \
    RETURN 1; \
    EXCEPTION WHEN VALUE_ERROR THEN RETURN 0;\
END isnumber;\
\
PROCEDURE p_refresh_snapshot(list varchar2, method varchar2:= '?')\
IS\
BEGIN	\
for obj in\
    (\
    SELECT * FROM user_mviews WHERE mview_name in upper(list)\
    ) loop\
        dbms_snapshot.refresh(obj.mview_name, method);\
    end loop;\
END p_refresh_snapshot;\
\
PROCEDURE p_compile_invalid_views\
IS\
    str varchar2(200);\
BEGIN\
for obj in\
    (\
    SELECT * FROM user_objects WHERE object_type='VIEW' and status = 'INVALID'\
    ) loop\
        str := 'alter view '||obj.object_name ||' compile';\
        EXECUTE IMMEDIATE str;\
    end loop;\
END p_compile_invalid_views;\
\
FUNCTION removeAccent(myString IN VARCHAR2) return varchar2\
IS\
/* Variables */\
rString VARCHAR2(1000);\
/* Body */\
BEGIN\
rString:=Translate(myString,'‡·‚„‰Â','aaaaaa');\
rString:=Translate(rString,'¿¡¬√ƒ≈','AAAAAA');\
